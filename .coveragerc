[run]
source = anyblok
omit =
    # omit scripts file
    feretui/scripts.py
    # omit anything in a tests directory anywhere
    */tests/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    # unexecuting logger
    logger\.
    # exclude all import error of unloaded module
    except ImportError:
    # exclude type checking
    if TYPE_CHECKING:
